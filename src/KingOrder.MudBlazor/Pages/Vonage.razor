@page "/vonage"
@inject IJSRuntime JS
@inject ISnackbar Snackbar

<PageTitle>Vonage</PageTitle>

<MudText Typo="Typo.h3" GutterBottom="true">Vonage</MudText>
<MudText Class="mb-4">Start a call to speak with support team.</MudText>
<MudButton Color="Color.Primary" Variant="Variant.Filled" @onclick="StartCall">Start Call</MudButton>


@code {
    private async void StartCall()
    {
        Snackbar.Add("Starting Vonage!");
        await JS.InvokeAsync<string>("startVonage");
    }
}
